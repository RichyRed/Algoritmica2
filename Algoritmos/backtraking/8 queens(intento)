#include <stdio.h>
#include <stdlib.h>
 
#define TRUE  1
#define FALSE 0

int verificar(int fila, int reinas[], int n) 
{
  int i;

  for (i=0; i<fila; i++)
      if (  ( reinas[i]==reinas[fila] )                      
         || ( abs(fila-i) == abs(reinas[fila]-reinas[i]) ) ) 
         return FALSE;

  return TRUE;
}
